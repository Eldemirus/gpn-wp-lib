{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "gpn-wp-lib": [
        "dist/gpn-wp-lib/gpn-wp-lib",
        "dist/gpn-wp-lib"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}